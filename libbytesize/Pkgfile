name=libbytesize
version=1.2
release=1
source=(https://github.com/rhinstaller/libbytesize/archive/$version/$name-$version.tar.gz)

build() {
	cd $name-$version
    ./autogen.sh
    ./configure --prefix=/usr \
            --without-gtk-doc 
        #    --with-python3
    make
    make DESTDIR=$PKG install
}
